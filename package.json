{
  "name": "ajv-napi",
  "version": "0.2.7",
  "description": "High-performance JSON Schema validator using NAPI-RS and Rust",
  "main": "wrapper.js",
  "exports": {
    ".": {
      "import": "./wrapper.mjs",
      "require": "./wrapper.js"
    }
  },
  "types": "index.d.ts",
  "files": [
    "wrapper.js",
    "wrapper.mjs",
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gauravsaini/ajv-napi.git"
  },
  "keywords": [
    "json",
    "schema",
    "validator",
    "ajv",
    "napi",
    "rust",
    "performance"
  ],
  "author": "gauravsaini",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gauravsaini/ajv-napi/issues"
  },
  "homepage": "https://github.com/gauravsaini/ajv-napi#readme",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "artifacts": "napi artifacts",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test.js && node test-features.js && node test-esm.mjs",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "napi": {
    "name": "ajv-napi",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
